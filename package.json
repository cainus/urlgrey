  ],
  "dependencies": {
    "fast-url-parser": "^1.1.3"
  },
  "devDependencies": {
    "coveralls": "^3.1.0",
    "istanbul": "^0.4.5",
    "mocha-lcov-reporter": "^1.3.0",
    "uglify-js": "^3.13.9",
    "chai": "^4.3.4",
    "jscoverage": "^0.6.0",
    "jshint": "^2.13.0",
    "mocha": "^9.0.0"
  },
  "engine": {
