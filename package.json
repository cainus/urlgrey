{
  "name": "urlgrey",
  "description": "urlgrey is a library for url querying and manipulation",
  "keywords": [
    "url",
    "uri"
  ],
  "version": "1.0.0",
  "bugs": {
    "url": "https://github.com/cainus/urlgrey/issues"
  },
  "scripts": {
    "test": "make test-coveralls"
  },
  "browser": {
    "url": "./browser/url.js",
